{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "vscode-jest-tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
            "args": ["--runInBand", "--coverage=false"],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
            }
    ]
}