{
  "name": "redux-db",
  "version": "0.9.17",
  "description": "A state normalizer and orm for redux",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "./src/types.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc && tsc -p tsconfig.cjs.json && tsc -p tsconfig.dist.json && gulp compress",
