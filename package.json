{
  "name": "redux-db",
  "version": "1.1.0-beta.0",
  "description": "A state normalizer and orm for redux",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "jsnext:main": "./dist/es/index.js",
  "scripts": {
    "test": "jest",
    "build": "gulp clean && yarn build:es && yarn build:cjs && yarn build:amd && gulp compress",
    "build:es": "tsc -p tsconfig.es.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:amd": "tsc -p tsconfig.dist.json",
    "build:gz": "gzip './dist/redux-db.min.js' -q -k -f",
    "docs": "rimraf \"docs/_build/\" && python -msphinx -b html \"docs\" \"docs/_build/html\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msolvaag/redux-db.git"
  },
  "keywords": [
    "redux",
    "database",
    "orm",
    "db",
    "normalize"
  ],
  "author": "msolvaag",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msolvaag/redux-db/issues"
  },
  "homepage": "https://github.com/msolvaag/redux-db#readme",
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.12.21",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-uglify": "^3.0.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "pump": "^3.0.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  }
}
