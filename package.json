    "url": "https://github.com/msolvaag/redux-db/issues"
  },
  "homepage": "https://github.com/msolvaag/redux-db#readme",
  "dependencies": {
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-uglify": "^3.0.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
