{
  "compileOnSave": false,
  "compilerOptions": {
    "noEmitOnError": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "removeComments": false,
    "target": "es5",
    "outDir": "./wwwroot/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "strictNullChecks": true,
    "allowJs": true,
    "alwaysStrict": true,
    "baseUrl": "./src",
    "diagnostics": false,
    "traceResolution": false
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}